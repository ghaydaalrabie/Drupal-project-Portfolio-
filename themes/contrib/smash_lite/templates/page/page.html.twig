{#
/**
 * @file
 * Theme override to display a single page.
 *
 * The doctype, html, head and body tags are not in this template. Instead they
 * can be found in the html.html.twig template in this directory.
 *
 * Available variables:
 *
 * General utility variables:
 * - base_path: The base URL path of the Drupal installation. Will usually be
 *   "/" unless you have installed Drupal in a sub-directory.
 * - is_front: A flag indicating if the current page is the front page.
 * - logged_in: A flag indicating if the user is registered and signed in.
 * - is_admin: A flag indicating if the user has permission to access
 *   administration pages.
 *
 * Site identity:
 * - front_page: The URL of the front page. Use this instead of base_path when
 *   linking to the front page. This includes the language domain or prefix.
 *
 * Page content (in order of occurrence in the default page.html.twig):
 * - messages: Status and error messages. Should be displayed prominently.
 * - node: Fully loaded node, if there is an automatically-loaded node
 *   associated with the page and the node ID is the second argument in the
 *   page's path (e.g. node/12345 and node/12345/revisions, but not
 *   comment/reply/12345).
 *
 * Regions:
 * - page.site_logo: Items for the header region.
 * - page.top_header: Items for the primary menu region.
 * - page.help: Dynamic help text, mostly for admin pages.
 * - page.content: The main content of the current page.
 * - page.footer_first: Items for the footer region.
 * - page.footer_second: Items for the footer region.
 * - page.footer_third: Items for the footer region.
 * - page.breadcrumb: Items for the breadcrumb region.
 *
 * @see template_preprocess_page()
 */
#}

<!-- Start: Header -->
<div class="dialog-off-canvas-main-canvas" data-off-canvas-main-canvas>
  <header class="header clear">
    <div class="header-cicle header-cicle1"></div>
    <div class="header-cicle header-cicle3"></div>
    <div class="header-cicle header-cicle4"></div>
    <div class="header-cicle header-cicle6"></div>
    <div class="header-cicle header-cicle7"></div>
    <div class="header-cicle header-cicle8"></div>
    <div class="header-cicle header-cicle10"></div>
    <div class="header-cicle header-cicle2"></div>
    <div class="header-cicle header-cicle5"></div>
    <div class="header-cicle header-cicle9"></div>
    <div class="container">
      <div class="header-main">
        <div class="site-brand">
          <div class="block-region region-site-branding">
            <div id="block-smash_lite-branding" class="contextual-region block">
              <div data-contextual-id="block:block=smash_lite_branding:langcode=en"
                data-contextual-token="KO3DE52Hl0A8085Q1wvB3bxHz9qwJ9zzrnJHrWhOECs"></div>
              <div class="block-content">
                <div class="site-branding">
                  <div class="site-logo">
                    {% if page.site_logo %}
                      {{ page.site_logo }}
                    {% endif %}
                  </div>
                </div>
              </div>
            </div>
            <!--/.block-content -->
          </div>
        </div>
        <!--/.site-branding -->
        <div class="header-main-right">
          <div class="mobile-menu"><i class="fa fa-bars"></i></div>
          <div class="primary-menu-wrapper">
            <div class="menu-wrap">
              <div class="close-mobile-menu">x</div>
              <div class="block-region region-primary-menu">
                <nav role="navigation" aria-labelledby="block-smash_lite-main-menu-menu" id="block-smash_lite-main-menu"class="contextual-region block block-menu">
                  <h2 class="visually-hidden block-title" id="block-smash_lite-main-menu-menu">Main navigation</h2>
                  <div data-contextual-id="block:block=smash_lite_main_menu:langcode=en|menu:menu=main:langcode=en"data-contextual-token="pcy94TLuiBEz7_xiY90zSn3rtcqhKhE24hBUzLj0Fig"></div>
                 {% if page.navigation %}  
                  <ul class="main-menu">
                   {{ page.navigation }}
                  </ul>
                 {% endif %}
                </nav>
              </div>
            </div> <!-- /.menu-wrap -->
          </div> <!-- /.primary-menu-wrapper -->

          <div class="full-page-search">
            <div class="search-icon">
              <i class="fa fa-search"></i>
            </div>
            <!--/.search icon -->
            <div class="search-box">
              <div class="search-box-close"></div>
              <div class="search-box-content">
                <div class="block-region region-search-box">
                  {% if page.header_search %}
                    <div class="search-block-form contextual-region block" data-drupal-selector="search-block-form"id="block-smash_lite-search" role="search">
                     {{ page.header_search }}
                    </div>
                  {% endif %}
                  <!--/.block-content -->
                </div>
              </div>
              <div class="search-box-close"></div>
            </div> <!-- /.search-box -->
          </div>
          <!--/.full-page-search -->
        </div> <!-- /.header-right -->
      </div><!-- /header-main -->
    </div><!-- /container -->
   {% if slideshow_display %}
    <section class="slider">
      <div class="container slider-container">
        <div class="slider-text">
          <ul class="js-rotating">
            {% for key, option in slide %}
            {% if loop.first %}
              <li>
                {% if option.slide_head %}
                <h1>{{ option.slide_head }}</h1>
                {% endif %}
                {% if option.slide_desc %}
                <p>{{ option.slide_desc }}</p>
                {% endif %}
                {% if option.slide_url or option.slide_link_text %}
                <a class="button-link" href="{{ option.slide_url }}">{{ option.slide_link_text }}</a>
                {% endif %}
              </li>
              {% else %}
              <li>
                {% if option.slide_head %}
                <h1>{{ option.slide_head }}</h1>
                {% endif %}
                {% if option.slide_desc %}
                <p>{{ option.slide_desc }}</p>
                {% endif %}
                {% if option.slide_url or option.slide_link_text %}
                <a class="button-link" href="{{ option.slide_url }}">{{ option.slide_link_text }}</a>
                {% endif %}
              </li>
            {% endif %}
            {% endfor %}
          </ul>
          <!--/.home-slider -->
        </div><!-- /slider-text -->
        {% if show_header_image_details %}
        {% if header_image %}
        <div class="slider-image">
          <img src="{{ header_image }}"/>
        </div><!-- /slider-image -->
        {% endif %}
        {% endif %}
      </div>
      <!--/.container -->
    </section>
   {% endif %}
  </header>
</div>
<!-- start: content -->
{% if page.content %}
 <div class="container">
  {{ page.content }}
 </div>
{% endif %}
<!-- End: content -->
  <!-- Start: Footer -->
  <!--footer-first -->
    <section id="footer-blocks">
        <div class="container footer-container clear">
           {% if show_footer_first_details %}
            <div class="footer-block">
                <div class="block-region region-footer-first">
                    <div id="block-footerabout" class="block">
                        <div class="block-content">
                            <div class="field field--name-body field--type-text-with-summary field--label-hidden field-item">
                             <div class="footer-logo">
                              {% if footer_image %}
                                <img src="{{ footer_image }}" />
                              {% endif %}
                             </div> 
                              {% if footer_text %}
                                <p>{{ footer_text }}</p>
                              {% endif %}
                            </div>
                        </div>
                    </div>
                    <!--/.block-content -->
                </div>
            </div>
           {% endif %}
            <!--footer-second -->
           <div class="footer-block">
              <div class="block-region region-footer-second">
                {% if page.footer_second %}
                  <div id="block-footersupport" class="block">
                    {{ page.footer_second }}
                  </div>
                {% endif %}
              </div>
           </div>
          <!--/footer-second -->

          <!--footer_third -->
            <div class="footer-block">
                <div class="block-region region-footer-third">
                  {% if page.footer_third %}
                    <div id="block-usefullinks" class="block">
                        {{ page.footer_third }}
                    </div>
                  {% endif %}
                    <!--/.block-content -->
                </div>
            </div>
            <!--/footer-three -->
          {% if show_contact_details %}
            <div class="footer-block last-footer-block">
                <div class="block-region region-footer-fourth">
                    <div id="block-footercontactus" class="block">
                      {% if footer_contact %}
                        <h2 class="block-title">{{footer_contact}}</h2>
                      {% endif %}
                        <div class="block-content">
                            <div class="field field--name-body field--type-text-with-summary field--label-hidden field-item">
                              {% if location %}
                                <p><i class="fa fa-map-marker theme-color" aria-hidden="true"></i>{{ location }}</p>
                              {% endif %}
                              {% if phone %}
                                <p><i class="fa fa-mobile theme-color" aria-hidden="true"></i>{{ phone }}</p>
                              {% endif %}
                              {% if emails %}
                                <p><i class="fa fa-envelope-o theme-color" aria-hidden="true"></i>{{ emails }}</p>
                              {% endif %}
                            </div>
                        </div>
                    </div>
                    <!--/.block-content -->
                </div>
            </div>
          {% endif %}
            <!--/footer-fourth -->
        </div>
        <!--/container -->
    </section>

  <!-- /footer-top -->
  <footer class="footer clear">
    <!-- /footer-bottom -->
    <section id="footer-bottom-last" class="clear">
        <div class="container clear">
            <div class="footer-bottom-last">
              {% if footer_copyright %}
                <div class="copyright">
                  {{ footer_copyright }}
                </div>
              {% endif %}
              {% if show_social_icon %}
                <div class="footer-social-icons">
                  <ul class="social-icons">
                    {% if facebook_url %}
                     <li><a href="{{ facebook_url }}" target="_blank"><i class="fa fa-facebook"></i></a></li>
                    {% endif %}
                    {% if twitter_url %}
                     <li><a href="{{ twitter_url }}" target="_blank"><i class="fa fa-twitter"></i></a></li>
                    {% endif %}
                    {% if instagram_url %}
                     <li><a href="{{ instagram_url }}" target="_blank"><i class="fa fa-instagram"></i></a></li>
                    {% endif %}
                    {% if linkedin_url %}
                     <li><a href="{{ linkedin_url }}" target="_blank"><i class="fa fa-linkedin"></i></a></li>
                    {% endif %}
                    {% if youtube_url %}
                     <li><a href="{{ youtube_url }}" target="_blank"><i class="fa fa-youtube"></i></a></li>
                    {% endif %}
                    {% if vimeo_url %}
                     <li><a href="{{ vimeo_url }}" target="_blank"><i class="fa fa-vimeo"></i></a></li>
                    {% endif %}
                    {% if telegram_url %}
                     <li><a href="{{ telegram_url }}" target="_blank"><i class="fa fa-telegram"></i></a></li>
                    {% endif %}
                    {% if whatsapp_url %}
                     <li><a href="{{ whatsapp_url }}" target="_blank"><i class="fa fa-whatsapp"></i></a></li>
                    {% endif %}
                    {% if github_url %}
                     <li><a href="{{ github_url }}" target="_blank"><i class="fa fa-github"></i></a></li>
                    {% endif %}
                  </ul>
                </div>
              {% endif %}
            </div>
            <!--/.social icons -->
        </div> <!-- /.container -->
    </section> <!-- /footer-bottom-last -->
<!-- end condition if copyright or social icons -->
  </footer>

  <!-- scrolltop start-->
  {% if show_scroller_details %}
    <div class="scrolltop {{ arrow_up }}">
      <div class="scrolltop-icon">&#x2191;</div>
    </div>
  {% endif %}
  <!-- scrolltop end-->
<!-- End: Footer -->
